#include <stdio.h>

int main()
{
	int a, b, c, d, n;
	
	printf("Ingrese cuatro dígitos para el entero N: ");
	scanf("%d%d%d%d", &a, &b, &c, &d);
	
	
	n = a * 1000 + b * 100 + c * 10 + d;
	
	
	n = ((n + 50) / 100) * 100;
	
	// Imprime N
	printf("El número redondeado es: %d\n", n);
	
	return 0;
}
